  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitest/ui": "^0.23.2",
    "javascript-yesterday": "^1.2.0",
    "jsdom": "^20.0.0",
